{
  "ifndef_header": {
    "prefix": "ifndef_header",
    "body": [
      "#ifndef __${1}_H__",
      "#define __${1}_H__",
      "",
      "#endif // __${1}_H__"
    ],
    "description": "header pragma once c style"
  },
  "std_string": {
    "prefix": "ss",
    "body": ["std::string"],
    "description": "prints 'std::string'"
  },
  "nolint": {
    "prefix": "no",
    "body": [" // NOLINT"],
    "description": "prints ' // NOLINT'"
  }
}
